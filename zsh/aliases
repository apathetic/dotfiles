alias grep="grep --color"
alias c="clear"
alias svim="sudo vim"

alias t='top -U <username> -O cpu -n 10'
alias ps='ps -fx'
alias p='ping www.google.com'
alias net='lsof -i ":80"'
alias ip="ifconfig | sed -En 's/127.0.0.1//;s/.*inet (addr:)?(([0-9]*\.){3}[0-9]*).*/\2/p'"
alias server='python -m SimpleHTTPServer'

